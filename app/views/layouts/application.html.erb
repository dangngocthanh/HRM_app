  <script src="https://kit.fontawesome.com/9cf65df64c.js" crossorigin="anonymous"></script>
</head>

<body>
<div class="container-fluid">
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
</div>
<div class="container">
  <%= yield %>
</div>
</body>
</html>
